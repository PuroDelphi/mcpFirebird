{
  "name": "mcp-firebird",
  "version": "1.0.59",
  "description": "Middleware Communication Protocol para Firebird que permite a los agentes de IA conectarse y realizar operaciones CRUD",
  "main": "dist/start-mcp-firebird.js",
  "types": "dist/types/mcp.d.ts",
  "bin": {
    "mcp-firebird": "./dist/start-mcp-firebird.js"
  },
  "scripts": {
    "start": "node dist/start-mcp-firebird.js",
    "dev": "nodemon src/start-mcp-firebird.ts",
    "build": "tsc",
    "test": "jest",
    "test:integration": "jest --config jest.integration.config.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "firebird",
    "mcp",
    "ai",
    "database",
    "middleware",
    "websocket",
    "typescript"
  ],
  "author": "Tu Nombre",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "node-firebird": "^1.1.3",
    "ws": "^8.16.0",
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.19",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tu-usuario/mcp-firebird.git"
  },
  "bugs": {
    "url": "https://github.com/tu-usuario/mcp-firebird/issues"
  },
  "homepage": "https://github.com/tu-usuario/mcp-firebird#readme"
}
